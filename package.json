{
  "name": "complete-intro-to-react-v5",
  "version": "1.0.0",
  "description": "the Complete Intro to React, v5",
  "homepage": "https://logan-anderson.github.io/quiz-app-v2/",
  "main": "src/App.js",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "postcss-modules": "^3.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "tailwindcss": "^1.5.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^14.0.23",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^5.2.0",
    "parcel-bundler": "^1.12.1",
    "prettier": "^1.16.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "predeploy": "rm -rf dist && parcel build src/index.html --public-url /quiz-app-v2",
    "deploy": "gh-pages -d dist",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "tslint --project .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Logan Anderson",
  "license": "Apache-2.0",
  "browserslist": [
    "last 2 Chrome versions"
  ]
}